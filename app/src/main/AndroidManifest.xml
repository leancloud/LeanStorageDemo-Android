<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.example.leancloud_demo"
    xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="com.example.leancloud_demo.DemoApplication"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light">
        <activity
            android:name="com.example.leancloud_demo.DemoGroupActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.leancloud_demo.demo.ObjectDemoActivity" />
        <activity android:name="com.example.leancloud_demo.demo.QueryDemoActivity" />
        <activity android:name="com.example.leancloud_demo.demo.FileDemoActivity" />
        <activity android:name="com.example.leancloud_demo.demo.UserDemoActivity" />
        <activity android:name="com.example.leancloud_demo.demo.SubclassDemoActivity" />
        <activity android:name="com.example.leancloud_demo.SourceCodeActivity" />
        <activity android:name="com.example.leancloud_demo.demo.PointerDemoActivity" />
        <activity android:name="com.example.leancloud_demo.demo.RelationDemoActivity" />
        <activity android:name="com.example.leancloud_demo.demo.EngineDemoActivity" />
        <activity android:name="com.example.leancloud_demo.demo.OtherDemoActivity" />
        <activity android:name="com.example.leancloud_demo.demo.UserAuthDataDemoActivity" />

        <activity android:name="ar.com.daidalos.afiledialog.FileChooserActivity" />
        <activity
            android:name="com.example.leancloud_demo.DemoRunActivity"
            android:label="@string/title_activity_demo_run"
            android:launchMode="singleTask" />
    </application>

</manifest>
